                </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>ChangetSet From</label>
                            @if (Model.DmdChangeSetDetails != null)
